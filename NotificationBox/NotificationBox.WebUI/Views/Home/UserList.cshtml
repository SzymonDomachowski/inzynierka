@model IEnumerable<NotificationBox.Domain.Entities.User>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>UserList</title>
</head>
<body>
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3>All Users</h3>
        </div>
        <div class="panel-body">
            <table class="table table-striped table-condensed table-bordered">
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Login</th>
                    <th>InstaToken</th>
                    <th>FacebookToken</th>
                    <th>Password</th>
                    <th>Akcje</th>
                </tr>
                @foreach (var item in Model)
            {
                    <tr>
                        <td>@item.UserID</td>
                        <td>@item.Name</td>
                        <td>@item.Login</td>
                        <td>@item.InstagramToken</td>
                        <td>@item.FacebookToken</td>
                        <th>@item.Password</th>
                        <td>

                        </td>
                    </tr>
                }

            </table>
        </div>
        <div class="panel-footer">
        </div>
    </div>
</body>
</html>
